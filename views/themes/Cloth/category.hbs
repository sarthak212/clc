<div class="uppercategory">
    <img src="/images/electrician.jpg" alt="">
</div>
 <section class="u-align-center u-clearfix u-grey-5 u-section-2" id="sec-85e1">
      <div class="u-clearfix u-sheet u-valign-middle u-sheet-1">
       
        <div class="u-list u-repeater u-list-1" style="padding: 10px 50px;">
          
         
          {{#each results}}
           <form action="/product/save" method="POST" style="max-width: 400px;">
          <div class="u-align-center u-container-style u-list-item u-repeater-item u-video-cover u-white u-list-item-2">
            <div class="u-container-layout u-similar-container u-container-layout-3">
              <img alt="" class="u-expanded-width u-image u-image-default u-image-2" src="{{this.productImage.path}}">
              <div class="u-align-center u-container-style u-expanded-width-lg u-expanded-width-md u-expanded-width-xl u-group u-video-cover u-group-2">
                <div class="u-container-layout u-valign-top u-container-layout-4">
                 <input type="hidden" name="productTitle" value="{{this.productTitle}}">
                 <input type="hidden" name="productPrice" value="{{this.productPrice}}">
                 <input type="hidden" name="productDescription" value="{{this.productDescription}}">
                  <h4 class="u-custom-font u-font-roboto-condensed u-text u-text-5" >{{this.productTitle}}</h4>
                  <h5 class="u-text u-text-palette-1-base u-text-6" name="productPrice">{{this.productPrice}} ₹</h5>
                  <p class="u-text u-text-default u-text-7" name="productDescription">{{this.productDescription}}</p>  
                </div>
                 <div class="u-custom-font u-font-roboto-condensed u-text u-text-5">
                    <button class="btn btn-primary btn-block btn-shadow" name="SubmitAndExit" value="{{this}}" type="submit">Book</button>
                  </div>
              </div>
            </div>
          </div>
          </form>
          {{/each}}

          
        </div>
          
      </div>
    </section>
